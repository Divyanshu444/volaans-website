    <!-- Enhanced seamless experience script -->
    <script>
        $(document).ready(function(){
            // Initialize AOS with custom settings
            AOS.init({
                duration: 800,
                easing: 'ease-in-out',
                once: true,
                mirror: false
            });
            
            // Enhanced seamless scrolling
            $(window).on('scroll', function() {
                var scrollPos = $(window).scrollTop();
                
                // Parallax effect for sections
                $('.section-transition').each(function() {
                    var $section = $(this);
                    var offset = $section.offset().top;
                    var distanceToScroll = scrollPos - offset;
                    var windowHeight = $(window).height();
                    
                    // Only apply effect when section is in viewport with some buffer
                    if (Math.abs(distanceToScroll) < windowHeight * 1.2) {
                        // Calculate parallax intensity based on section position
                        var parallaxIntensity = 0.05;
                        
                        // Apply subtle movement to container elements
                        $section.find('.container').css({
                            'transform': 'translateY(' + (distanceToScroll * parallaxIntensity) + 'px)'
                        });
                        
                        // For sections with background images, create subtle parallax
                        if ($section.css('background-image') !== 'none') {
                            var bgPosY = (distanceToScroll * 0.03);
                            $section.css('background-position', 'center ' + bgPosY + 'px');
                        }
                        
                        // Apply different effects to different types of elements
                        $section.find('.img1, .images').css({
                            'transform': 'translateY(' + (distanceToScroll * -0.02) + 'px) scale(' + (1 + Math.abs(distanceToScroll) * 0.0001) + ')'
                        });
                        
                        // Fade effect for elements as they enter/leave viewport
                        var opacityValue = 1 - (Math.abs(distanceToScroll) / windowHeight);
                        opacityValue = Math.max(0.5, Math.min(1, opacityValue));
                        
                        // Apply smooth transitions between sections
                        $section.find('h2, h5, .heading1').css({
                            'opacity': opacityValue,
                            'transform': 'translateY(' + (distanceToScroll * 0.02) + 'px)'
                        });
                    }
                });
            });
            
            // Trigger scroll event on page load to initialize effects
            $(window).trigger('scroll');
            
            // Smooth scroll for anchor links
            $('a[href^="#"]').on('click', function(e) {
                var target = $(this.getAttribute('href'));
                if (target.length) {
                    e.preventDefault();
                    $('html, body').animate({
                        scrollTop: target.offset().top
                    }, 1000);
                }
            });
            
            // Industry grid animations
            function initIndustryGrid() {
                $('.industry-grid-item').each(function(index) {
                    var $item = $(this);
                    setTimeout(function() {
                        $item.css({
                            'opacity': '1',
                            'transform': 'translateY(0)'
                        });
                    }, index * 100);
                });
            }
            
            // Initialize when element is in viewport
            if ('IntersectionObserver' in window) {
                var observer = new IntersectionObserver(function(entries) {
                    entries.forEach(function(entry) {
                        if (entry.isIntersecting) {
                            if ($(entry.target).hasClass('serve-section-area')) {
                                initIndustryGrid();
                            }
                        }
                    });
                }, {threshold: 0.1});
                
                // Observe industry grid section
                if ($('.serve-section-area').length) {
                    observer.observe(document.querySelector('.serve-section-area'));
                }
            }
            
            // Initialize sliders and carousels
            $('.about-brand-slider').owlCarousel({
                loop: true,
                margin: 30,
                nav: false,
                dots: false,
                autoplay: true,
                autoplayTimeout: 3000,
                responsive: {
                    0: { items: 2 },
                    576: { items: 3 },
                    768: { items: 4 },
                    992: { items: 5 }
                }
            });
        });
    </script>
