    <!-- Seamless Container Styles -->
    <style>
        /* Main seamless container */
        .seamless-container {
            position: relative;
            overflow: hidden;
        }
        
        /* Remove visible divisions between sections */
        .seamless-container > div {
            position: relative;
            margin-top: 0 !important;
            margin-bottom: 0 !important;
            transition: transform 0.5s ease;
            background-size: cover !important;
            background-position: center !important;
        }
        
        /* Smooth scrolling for entire page */
        html {
            scroll-behavior: smooth;
        }
        
        /* Section transition styling */
        .section-transition {
            position: relative;
            padding: 70px 0;
        }
        
        .section-transition::after {
            content: '';
            position: absolute;
            left: 0;
            right: 0;
            bottom: -1px;
            height: 30px;
            background: linear-gradient(to bottom, rgba(0,0,0,0) 0%, rgba(0,0,0,0.03) 100%);
            z-index: 1;
            pointer-events: none;
        }
        
        /* Animation improvements */
        .animated-title {
            position: relative;
            display: inline-block;
            margin-bottom: 30px;
            padding-bottom: 20px;
        }
        
        .animated-title::after {
            content: '';
            position: absolute;
            bottom: 0;
            left: 0;
            width: 0;
            height: 3px;
            background: #0000fb;
            animation: lineGrow 1.5s ease-out forwards;
        }
        
        @keyframes lineGrow {
            to { width: 100%; }
        }
        
        /* Industry grid improvements */
        .industry-grid-item {
            transition: transform 0.4s ease, background-color 0.4s ease, box-shadow 0.4s ease, opacity 0.8s ease !important;
            overflow: hidden;
            border: 1px solid rgba(255, 255, 255, 0.2);
            background: rgba(255, 255, 255, 0.1);
            opacity: 0;
            transform: translateY(30px);
            will-change: opacity, transform;
        }
        
        .industry-grid-item:hover {
            transform: translateY(-10px) !important;
            background: rgba(0, 0, 251, 0.2) !important;
            box-shadow: 0 10px 25px rgba(0, 0, 251, 0.3) !important;
            border-color: rgba(0, 0, 251, 0.3) !important;
        }
    </style>

